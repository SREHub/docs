func quicsort(A []int) {
     i,mid := 1,A[0]
     head,tail := 0,len(A)-1
     for head < tail {
         if A[i] > mid {
            A[i],A[tail] = A[tail],A[i]
            tail--
         } else {
           A[i],A[head] = A[head],A[i]
           head++
           i++
         }
     }
     sort(A[:head])
     sort(A[head+1:])
}
